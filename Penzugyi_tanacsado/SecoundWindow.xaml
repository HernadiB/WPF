<Window x:Class="Penzugyi_tanacsado.SecoundWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Penzugyi_tanacsado"
        xmlns:model="clr-namespace:Penzugyi_tanacsado.Model"
        mc:Ignorable="d"
        Title="SecoundWindow" Height="450" Width="800">
    <Window.DataContext>
        <model:SecondWindowViewModel/>
    </Window.DataContext>
    <StackPanel>
        <Label Content="Új találkozó felvétele" Name="label1"/>
        <Label Content="Tanácsadó neve:"/>
        <ComboBox Name="cb1"/>
        <Label Content="Ügyfél neve:"/>
        <ComboBox Name="cb2"/>
        <Label Content="Találkozó dátuma:"/>
        <Calendar Name="calendar1"/>
        <Label Content="Találkozó ideje:"/>
        <ComboBox Name="cb3"/>
        <Button Content="Küldés" Name="btn" Click="btn_Click"/>
    </StackPanel>
</Window>
